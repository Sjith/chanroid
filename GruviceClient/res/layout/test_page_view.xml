<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_height="fill_parent" android:layout_width="fill_parent" android:orientation="vertical">
<TextView android:text="@+id/TextView01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/tv_argosStatus"></TextView><Button android:layout_height="wrap_content" android:id="@+id/btnGetMsg" android:text="GetMsg" android:layout_width="fill_parent"></Button>





<Button android:layout_height="wrap_content" android:id="@+id/BtnArgosStart" android:text="Argos_Start" android:layout_width="fill_parent"></Button>
<Button android:layout_height="wrap_content" android:id="@+id/BtnArgosStop" android:text="Argos_Stop" android:layout_width="fill_parent"></Button><EditText android:layout_height="wrap_content" android:inputType="numberDecimal" android:text="40" android:layout_width="fill_parent" android:id="@+id/etInterval"></EditText>


<Button android:layout_height="wrap_content" android:id="@+id/BtnRepeatSnm" android:text="Repeat Send Msg" android:layout_width="fill_parent"></Button>
<Button android:layout_height="wrap_content" android:id="@+id/BtnStopSnm" android:text="StopRepeatSnm" android:layout_width="fill_parent"></Button>
</LinearLayout>
