<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="wrap_content" android:orientation="horizontal"
	android:layout_width="fill_parent">


	<RelativeLayout android:id="@+id/gmRL01"
		android:layout_height="wrap_content" android:layout_width="fill_parent">
		<TextView android:text="@+id/TextView01"
			android:layout_width="wrap_content" android:id="@+id/gmTvName" android:textSize="20dip"
			android:layout_height="wrap_content" android:textColor="@color/v2_list_text_color"></TextView>
		<TextView android:text="@+id/TextView01"
			android:layout_width="wrap_content" android:id="@+id/gmTvDesc"
			android:layout_toRightOf="@+id/gmCtvID" android:textColor="@color/v2_list_text_color"
			android:layout_below="@+id/gmTvName" android:layout_height="wrap_content"
			android:textSize="12dip"></TextView>
	</RelativeLayout>

</LinearLayout>
