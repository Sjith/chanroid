<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content"
	android:layout_height="45dip">

	<RelativeLayout
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
	android:id="@+id/RelativeLayout01" android:layout_centerHorizontal="true">
		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/eiIvicon"
			android:background="@drawable/ic_el_list"
		android:layout_centerVertical="true" android:layout_margin="5dip" android:visibility="gone"></ImageView>
		<LinearLayout
			android:id="@+id/LinearLayout01"
			android:layout_height="wrap_content"
			android:orientation="vertical" android:layout_toRightOf="@+id/eiIvicon" android:layout_toLeftOf="@+id/LinearLayout03" android:layout_width="fill_parent" android:layout_marginLeft="5dip" android:layout_centerVertical="true">
			<TextView
				android:text="@+id/TextView01"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/eiTvTest"
				android:visibility="gone"
			></TextView>
			<TextView
				android:text="@+id/TextView01"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/eiTvName"
				android:textStyle="bold" android:textSize="17sp" android:textColor="#5A7F00"></TextView>
			<TextView
				android:text="@+id/TextView01"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/eiTvAddress" android:textColor="#666666"></TextView>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/LinearLayout03"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
		android:layout_alignParentRight="true" android:layout_centerVertical="true">
			<ImageView android:id="@+id/ImageView01" android:layout_height="fill_parent" android:layout_width="1px" android:layout_margin="3dip" android:background="#c0d990"></ImageView><Button
				android:id="@+id/eiBtnHomePage"
				android:background="@drawable/btn_home"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content"
			android:visibility="gone"></Button>
			<Button
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/eiBtnCall"
				android:background="@drawable/btn_tel"
			android:layout_marginRight="10dip" android:visibility="gone"></Button>
			<Button
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:id="@+id/eiBtnMap"
				android:background="@drawable/icon"
				android:visibility="gone"
			></Button>
		<CheckBox android:id="@+id/CheckBox" android:layout_gravity="center" android:visibility="gone" android:button="@drawable/checkbox" android:layout_height="45dip" android:layout_width="45dip" android:layout_margin="5dip"></CheckBox><ImageView android:id="@+id/eiBullet" android:layout_margin="10dip" android:background="@drawable/btn_view" android:layout_height="35dip" android:layout_width="35dip" android:layout_gravity="center"></ImageView>


</LinearLayout>
	</RelativeLayout>
<ImageView android:id="@+id/ImageView02" android:layout_below="@id/RelativeLayout01" android:layout_width="fill_parent" android:background="#c0d990" android:layout_height="1px"></ImageView>
</RelativeLayout>
