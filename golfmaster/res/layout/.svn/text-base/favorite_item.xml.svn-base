<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:background="#00000000"
	android:paddingLeft="40dip"
	android:orientation="vertical" android:layout_height="wrap_content">


	
	<LinearLayout android:layout_height="wrap_content" android:id="@+id/fiLlLocationCategory" android:layout_width="fill_parent" android:visibility="visible"><LinearLayout android:layout_height="30dip" android:visibility="visible" android:id="@+id/fiLlLocationCategory2" android:layout_width="fill_parent">
		
		<TextView android:text="@+id/TextView01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/TvLocationCategory" android:padding="5dip" android:textSize="15sp" android:textColor="#9D8500"></TextView>
	</LinearLayout>
</LinearLayout><LinearLayout
		android:id="@+id/LinearLayout01"
		android:layout_width="fill_parent"
		android:paddingLeft="15dip" android:layout_height="18dip" android:layout_marginBottom="7dip">
		<ImageView
			android:id="@+id/ImageView01"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:background="@drawable/bullet_05"
			android:layout_gravity="center_vertical" android:layout_marginTop="3dip"></ImageView>
		<TextView
			android:text="@+id/TextView01"
			android:id="@+id/fiTvName"
			android:textColor="#000000"
			android:layout_width="fill_parent"
			android:singleLine="true"
			android:textSize="15sp"
			android:onClick="onClick"
			android:layout_marginLeft="10dip" android:layout_height="wrap_content"></TextView>
	
</LinearLayout>


</LinearLayout>
