<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
			android:orientation="vertical" android:layout_width="match_parent"
			android:layout_height="match_parent">

			<LinearLayout android:orientation="vertical"
						android:layout_width="match_parent" android:layout_height="match_parent">

						<LinearLayout android:layout_width="match_parent"
									android:layout_height="match_parent" android:orientation="vertical">
									<LinearLayout android:layout_width="match_parent"
												android:layout_height="wrap_content" android:orientation="vertical"
												android:weightSum="1">


												<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
															android:layout_height="wrap_content" android:gravity="center"
															android:layout_width="fill_parent" android:orientation="horizontal">


												</LinearLayout>

												<RelativeLayout android:background="@drawable/bg_detail"
															android:layout_width="match_parent"
															android:layout_height="wrap_content">

															<LinearLayout android:layout_width="wrap_content"
																		android:id="@+id/clubSearchll1"
																		android:layout_centerVertical="true" android:gravity="center_vertical"
																		android:layout_height="wrap_content">
																		<Button android:id="@+id/searchestname"
																					android:onClick="onClick" android:background="@drawable/btn_detail_store"
																					android:layout_width="wrap_content"
																					android:layout_height="wrap_content"></Button>
																		<ImageView android:layout_width="wrap_content"
																					android:layout_height="wrap_content" android:src="@drawable/dot_detail"></ImageView>
																		<Button android:id="@+id/searchregionname"
																					android:onClick="onClick" android:background="@drawable/btn_detail_local"
																					android:layout_width="wrap_content"
																					android:layout_height="wrap_content"></Button>
																		<ImageView android:layout_width="wrap_content"
																					android:layout_height="wrap_content" android:src="@drawable/dot_detail"></ImageView>
																		<Button android:id="@+id/searchestkind"
																					android:onClick="onClick" android:background="@drawable/btn_detail_type"
																					android:layout_width="wrap_content"
																					android:layout_height="wrap_content"></Button>
																		<ImageView android:layout_width="wrap_content"
																					android:layout_height="wrap_content" android:src="@drawable/dot_detail"></ImageView>
																		<Button android:id="@+id/searchinit"
																					android:onClick="onClick" android:background="@drawable/btn_detail_reset"
																					android:layout_width="wrap_content"
																					android:layout_height="wrap_content"></Button>
															</LinearLayout>


															<LinearLayout android:layout_width="wrap_content"
																		android:layout_height="wrap_content"
																		android:layout_alignParentRight="true"
																		android:layout_centerVertical="true">
																		<Button android:id="@+id/searchButton1"
																					android:layout_width="wrap_content"
																					android:layout_height="wrap_content"
																					android:background="@drawable/btn_detail_search"
																					android:onClick="onClick"></Button>

															</LinearLayout>

												</RelativeLayout>

									</LinearLayout>


									<ListView android:layout_height="wrap_content"
												android:id="@+id/searchlist1" android:layout_width="match_parent"></ListView>
						</LinearLayout>

			</LinearLayout>


			<ProgressBar android:layout_width="wrap_content" android:layout_gravity="center" android:visibility="gone"
						android:layout_height="wrap_content" style="?android:attr/progressBarStyleLarge"
						android:id="@+id/searchloading"></ProgressBar>


</FrameLayout>
