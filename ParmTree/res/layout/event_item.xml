<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:id="@+id/eventBack"
	android:layout_width="match_parent" android:layout_height="match_parent">

	<ImageView android:id="@+id/eventBanner" android:src="@drawable/icon"
		android:layout_width="wrap_content" android:layout_height="wrap_content" />

	<TextView android:id="@+id/eventEstname" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textSize="22sp"
		android:text="업체명" android:layout_toRightOf="@id/eventBanner" />
	<TextView android:id="@+id/eventEnabledate"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:layout_alignBottom="@id/eventBanner" android:textSize="14sp"
		android:text="유효기간" android:layout_toRightOf="@id/eventBanner"
		android:layout_below="@id/eventEstname" />


</RelativeLayout>
