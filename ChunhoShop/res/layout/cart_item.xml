<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
			android:orientation="vertical" android:background="#ffffff"
			android:layout_width="match_parent" android:layout_height="wrap_content">
			<LinearLayout android:layout_width="wrap_content"
						android:id="@+id/linearLayout2" android:background="@drawable/list_box"
						android:layout_height="wrap_content" android:orientation="vertical"
						android:layout_centerVertical="true">
						<ImageView android:id="@+id/carticon"
									android:layout_height="90dp" android:layout_width="90dp"
									android:layout_margin="10dp" android:src="@drawable/icon"></ImageView>
			</LinearLayout>
			<LinearLayout android:layout_marginLeft="10dp"
						android:layout_marginRight="10dp" android:layout_marginTop="4dp"
						android:layout_width="wrap_content"
						android:id="@+id/linearLayout1" android:layout_height="wrap_content"
						android:orientation="vertical" android:layout_alignParentRight="true"
						android:layout_alignParentTop="true" android:layout_toRightOf="@+id/linearLayout3">
						<TextView android:text="상품명" android:layout_width="match_parent"
									android:textColor="#000000" android:maxLines="2"
									android:textSize="16sp" android:layout_height="wrap_content"
									android:id="@+id/cartname"></TextView>
						<LinearLayout android:layout_width="match_parent"
									android:layout_height="wrap_content" android:id="@+id/linearLayout4"
									android:gravity="center_vertical">
									<ImageView android:layout_width="wrap_content"
												android:layout_height="wrap_content" android:src="@drawable/list_btn_01"
												android:id="@+id/imageView1"></ImageView>
									<TextView android:textColor="#000000"
												android:layout_height="wrap_content" android:id="@+id/cartprice"
												android:layout_width="wrap_content" android:text="가격"
												android:textAppearance="?android:attr/textAppearanceSmall"></TextView>
						</LinearLayout>
						<LinearLayout android:layout_width="match_parent"
									android:layout_height="wrap_content" android:id="@+id/linearLayout5"
									android:gravity="center_vertical">
									<ImageView android:layout_width="wrap_content"
												android:layout_height="wrap_content" android:src="@drawable/list_btn_02"
												android:id="@+id/imageView2"></ImageView>
									<TextView android:textColor="#000000"
												android:layout_height="wrap_content" android:id="@+id/cartpoint"
												android:layout_width="wrap_content" android:text="적립금"
												android:textAppearance="?android:attr/textAppearanceSmall"></TextView>
						</LinearLayout>
			</LinearLayout>
			<LinearLayout android:layout_width="wrap_content"
						android:gravity="center" android:layout_marginLeft="10dp"
						android:id="@+id/linearLayout3" android:layout_height="wrap_content"
						android:orientation="vertical" android:layout_alignTop="@+id/linearLayout1"
						android:layout_toRightOf="@+id/linearLayout2" android:layout_alignBottom="@+id/linearLayout2">
						<Button android:id="@+id/cartplusbtn" android:background="@drawable/cart_puls"
									android:layout_width="wrap_content" android:layout_height="wrap_content"></Button>
						<TextView android:text="1" android:id="@+id/cartcount"
									android:textColor="#000000" android:layout_margin="5dp"
									android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
						<Button android:id="@+id/cartminusbtn" android:background="@drawable/cart_minus_btn"
									android:layout_width="wrap_content" android:layout_height="wrap_content"></Button>
			</LinearLayout>
			<LinearLayout android:orientation="vertical" android:layout_height="wrap_content"
						android:gravity="bottom" android:layout_width="wrap_content" android:paddingBottom="3dp"
						android:id="@+id/linearLayout6" android:layout_alignParentBottom="true" android:layout_alignLeft="@+id/linearLayout1" android:layout_below="@+id/linearLayout1">
						<LinearLayout android:layout_height="wrap_content"
									android:layout_width="wrap_content" android:id="@+id/linearLayout7">
									<ImageView android:layout_height="wrap_content"
												android:layout_width="wrap_content" android:src="@drawable/list_s4"
												android:id="@+id/carthealthflag"></ImageView>
									<ImageView android:layout_height="wrap_content"
												android:layout_width="wrap_content" android:src="@drawable/list_s5"
												android:id="@+id/cartoptionflag"></ImageView>
						</LinearLayout>
						<TextView android:text="TextView" android:layout_width="wrap_content" android:id="@+id/cartoptiontext" android:layout_marginTop="3dp" android:maxLines="1" android:layout_height="wrap_content" android:textSize="10sp" android:visibility="visible" android:textColor="#000000" android:layout_marginLeft="3dp"></TextView>
			</LinearLayout>

</RelativeLayout>
