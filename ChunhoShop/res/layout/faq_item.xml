<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
			android:orientation="vertical" android:background="#efefef"
			android:padding="5dp" android:layout_width="match_parent"
			android:layout_height="wrap_content">
			<TextView android:layout_height="wrap_content" android:id="@+id/faqitemtitle"
						android:textSize="18sp" android:layout_width="wrap_content"
						android:text="TextView" android:textColor="#000000" android:textStyle="bold"
						android:layout_centerVertical="true" android:maxLines="1"
						android:layout_alignParentLeft="true" android:layout_toLeftOf="@+id/imageView1"></TextView>
			<ImageView android:id="@+id/imageView1"
						android:layout_width="wrap_content" android:src="@drawable/center_arrow"
						android:layout_height="wrap_content"
						android:layout_centerVertical="true"
						android:layout_alignParentTop="true" android:layout_margin="3dp"
						android:layout_alignParentRight="true"></ImageView>
			<ImageView android:layout_width="wrap_content" android:id="@+id/imageView2"
						android:layout_height="70dp" android:layout_toLeftOf="@id/imageView1"></ImageView>

</RelativeLayout>
